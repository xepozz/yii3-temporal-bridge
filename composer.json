{
  "name": "xepozz/yii3-temporal-bridge",
  "authors": [
    {
      "name": "Dmitrii Derepko",
      "email": "xepozz@list.ru"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-grpc": "*",
    "temporal/sdk": "^2.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.4",
    "yiisoft/classifier": "dev-master",
    "yiisoft/di": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Xepozz\\Yii3\\Temporal\\Bridge\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Xepozz\\Yii3\\Temporal\\Bridge\\Tests\\": "tests/"
    }
  },
  "extra": {
    "config-plugin-options": {
      "source-directory": "config"
    },
    "config-plugin": {
      "params": "params.php",
      "di": "di.php",
      "tags": "tags.php"
    }
  },
  "config": {
    "sort-packages": true
  }
}
